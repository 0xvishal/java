         {
           sh """ssh -o StrictHostKeyChecking=no -tt ubuntu@$cli << EOF 
              ## creating aws Instance
              cat > outfile.txt <<EOF
               sudo apt-get update
               # this is file
              EOF 
              
              exit
           EOF"""
         }
        
      }
  
   }
}
