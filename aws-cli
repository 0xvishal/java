   def ins = '34.204.2.87'   
   stage('Install Docker'){
      sshagent(['tomcat-dev']) {  
         //sh 'scp -o StrictHostKeyChecking=no target/*.war ubuntu@34.204.2.87:/var/lib/tomcat8/webapps/ROOT/'
